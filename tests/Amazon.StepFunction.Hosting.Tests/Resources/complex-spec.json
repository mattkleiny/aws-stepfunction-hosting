{
  "Comment": "A more complex example of the States language",
  "StartAt": "Step1",
  "States": {
    "Step1": {
      "Type": "Task",
      "Resource": "format-message",
      "Next": "Step2"
    },
    "Step2": {
      "Type": "Wait",
      "Seconds": 2,
      "Next": "Step3"
    },
    "Step3": {
      "Type": "Task",
      "Resource": "capitalize-message",
      "Next": "Step4"
    },
    "Step4": {
      "Type": "Task",
      "Resource": "print-message",
      "End": "true"
    }
  }
}